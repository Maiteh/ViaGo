<?php
/**
 * @file
 * Holds the schema for a hms_field.
 */

/**
 * Implements hook_field_schema().
 */
function hms_field_field_schema($field) {
  $columns = array(
    'value' => array(
      'type' => 'int',
      'unsigned' => FALSE,
      'not null' => TRUE,
      'default' => 0,
    ),
  );
  return array(
    'columns' => $columns,
  );
}
